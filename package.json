{
  "name": "rocks",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "flexboxgrid": "^6.3.1",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src -o public/css/ --recursive",
    "watch-css": "npm run build-css && node-sass-chokidar src -o public/css/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "deploy": "npm run build && gh-pages -d build -b master"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "webpack": "^3.8.1"
  },
  "gh-pages-deploy": {}
}
